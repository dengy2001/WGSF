	<class name="Charge" table="Charge">
		<id name="CID" column="CID" >
			<generator class="increment" />
		</id>
		<property name="PeriodNo" column="PeriodNo"  />
		<property name="ChargeTotal" column="ChargeTotal"   />
		<property name="Abstract" column="Abstract"   />
